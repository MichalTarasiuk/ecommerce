{
  "name": "ecommerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "make-i18n": "pnpm tsc --module CommonJS --target ESNext --resolveJsonModule true i18n && pnpm prettier --write i18n.js",
    "dev": "pnpm make-i18n && next dev",
    "build": "pnpm make-i18n && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "format": "prettier --write .",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless",
    "generate": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@next/font": "13.1.4",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.4",
    "graphql": "^16.6.0",
    "next": "13.1.4",
    "next-translate": "^1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4",
    "urql": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/client-preset": "1.2.6",
    "@graphql-typed-document-node/core": "^3.1.1",
    "autoprefixer": "^10.4.13",
    "cosmiconfig-toml-loader": "^1.0.0",
    "cypress": "^12.3.0",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1"
  }
}
